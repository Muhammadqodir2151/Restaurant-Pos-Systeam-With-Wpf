<Application x:Class="Restaurant_Pos_Systeam_With_Wpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Restaurant_Pos_Systeam_With_Wpf"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--begin::Themes-->
                <ResourceDictionary Source="/Themes/LightTheme.xaml"/>
                <!--end::Themes-->
                
                <!--begin::Icons-->
                <ResourceDictionary Source="/Assets/Icons/Icons.xaml"/>
                <!--end::Themes-->

                <!--begin::Styles-->
                <ResourceDictionary Source="/Stayles/ButtonStyles.xaml"/>

                <ResourceDictionary Source="/Stayles/ComboboxStyles.xaml"/>
                <!--end::Styles-->

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
